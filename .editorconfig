# c# 文件
[*.cs]

# CA2211: 非常量字段应当不可见
dotnet_diagnostic.CA2211.severity = none

#### Core EditorConfig 选项 ####

# 新行首选项
insert_final_newline = true

# IDE0290: 使用主构造函数
dotnet_diagnostic.IDE0290.severity = none

# CS8618: 在退出构造函数时，不可为 null 的字段必须包含非 null 值。请考虑声明为可以为 null。
dotnet_diagnostic.CS8618.severity = suggestion

# CS8603: 可能返回 null 引用。
dotnet_diagnostic.CS8603.severity = suggestion

# CS8600: 将 null 字面量或可能为 null 的值转换为非 null 类型。
dotnet_diagnostic.CS8600.severity = suggestion
